			}
			antiCheatCS[ playerid ][ TrueHealth ] = Zivot;
		}
		
		// -- Anti Vehicle Repair
		if( antiCheatSS[ antiVehRepair ] == true ) {
		    if( antiCheatCS[ playerid ][ VehRepairTime ] <= 0 ) {
				if( GetPlayerState( playerid ) == PLAYER_STATE_DRIVER ) {
