public OnPlayerCommandReceived( playerid, cmdtext[])
{
	if(!IsPlayerConnected( playerid )) return 0;

    if( PlayerInfo[ playerid ][ xAdmin ] < 1 && PlayerInfo[ playerid ][ xSupporter ] < 1 ) {
	    if( antiCheatCS[ playerid ][ SpamCommand ] > 0) {
		    SCM( playerid, SVETLOCRVENA, "| SM:AC | Ne mozes spamati komande svakih 2 sec!");
			return 0;
		}
		antiCheatCS[ playerid ][ SpamCommand ] += 2;
 	}

	if( PlayerLogged[ playerid ] != true )
	{
	    SendErrorMessage( playerid, "Trebate se prijaviti da bi ste mogli koristit ovu komandu !");
		return 0;
	}
	return 1;
}

public OnPlayerCommandPerformed( playerid, cmdtext[], success) {

	if( !success ) return SendErrorMessage( playerid, "Upisali ste komandu koja ne postoji( popis pogledajte na /help ).");

 
	new string[ 128 ]; format( string, sizeof( string ), "Igrac %s | Komanda: %s", ImeIgraca( playerid ), cmdtext ); LogSave( "Ostalo/LogKomandi.log", string );

	return true;
}

