			format( string ,sizeof( string ),"~w~Ubistva: ~g~%d~n~~w~Smrti: ~r~%d~n~~w~Igraci: ~y~%d", WARInfo[ w ][ WARTimSkor ][ 0 ], WARInfo[ w ][ WARTimSkor ][ 1 ], WARInfo[ w ][ WARTimClanovi ][ 0 ] );
