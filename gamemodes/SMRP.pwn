	}
	else if( strcmp( item, "radnici",true) == 0) {
	    SCM( playerid, 0xF5DEB3AA, "|----------------Zaposleni Online----------------|");
	    foreach( Player, i ) {
			if( IsPlayerConnected( i ) ) {
			    new string[ 50 ];
				if( PlayerInfo[ i ][ xGradRank ] == GRAD_MAYOR ) format( string, sizeof( string ), "Gradonacelnik - %s", ImeIgraca( i ) );
