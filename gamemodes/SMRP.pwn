							PorukaRadius( 30.0, i, globalstring, LJUBICASTA );
							SetPVarInt( playerid, "CellTime",  1 );
							return 1;
						}
					}
				}
			}
		}
	}
	SCM( playerid, CRVENA, "  Zauzeto...");
	return 1;
}

