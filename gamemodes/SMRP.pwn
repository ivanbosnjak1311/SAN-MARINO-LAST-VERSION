		if( Pos[ 0 ] <= fieldInfo[ id ][ fieldMaxX ] && Pos[ 0 ] >= fieldInfo[ id ][ fieldMinX ] && Pos[ 1 ] <= fieldInfo[ id ][ fieldMaxY ] && Pos[ 1 ] >= fieldInfo[ id ][ fieldMinY ] ) {
