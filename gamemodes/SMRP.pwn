	    if( atmInfo[ atmid ][ atmVremeDoRoba ] != 0 ) return SendErrorMessage( playerid, "Ovaj bankomat je vec opljackan." );
		atmTDControl( playerid, true );
		SelectTextDraw( playerid, SANMARINO );
