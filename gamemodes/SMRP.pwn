			cistackilometri[ playerid ] = 0;
			cistacmetri[ playerid ] = 0;
			cistacnovac[ playerid ] = 0;
			CistacWarning[ playerid ] = 0;
            Dimi_KillTimer( CistacTimer[ playerid ] );
