				if( GetPlayerSurfingVehicleID( playerid ) == INVALID_VEHICLE_ID && !IsVehicleLetelica( GetPlayerVehicleID( playerid ) ) && !antiSpeedVehicle( GetPlayerVehicleID( playerid ) ) ) {
