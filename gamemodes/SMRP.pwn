        PraznaGajbica[ playerid ] = false;
        if( IsPlayerAttachedObjectSlotUsed( playerid, OBJECT_NOSI ) ) RemovePlayerAttachedObject( playerid, OBJECT_NOSI );
        
		if( stalkInfo[ id ][ sType ] == BILJKA_SLJIVA ) {
