stock Stalk_Delete( id ) {
	new filename[ 32 ];
	
	if( id  != -1 ) {
	
	    format( filename, sizeof( filename ), F_STALK, id );

		if( fexist( filename ) ) {
		
		    if( IsValidDynamic3DTextLabel( stalkInfo[ id ][ s3D ] ) )
