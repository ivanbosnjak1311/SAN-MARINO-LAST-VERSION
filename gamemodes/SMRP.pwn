			SendClientMessage( playerid, CRVENA, "REGISTRACIJA JE TRENUTNO ONEMOGUCENA." );
			SendClientMessage( playerid, CRVENA, "SAN MARINO ASM TEAM." );
			SetTimerEx( "KickIgraca", 150, false, "d", playerid );
            return 1;
	    }
	    
