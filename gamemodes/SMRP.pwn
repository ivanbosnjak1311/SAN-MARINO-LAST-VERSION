	    if( UzeoOpremu[ playerid ] == false ) return SendErrorMessage( playerid, "Nemate opremu!" );
		if( AccountVerified[ playerid ] == false ) return SendErrorMessage( playerid, "Prvo morate verifikovati svoj nalog(/verifikacija).");
