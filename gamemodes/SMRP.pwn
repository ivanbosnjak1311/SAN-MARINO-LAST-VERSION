	if( IsPlayerInAnyVehicle( playerid ) ) {
		Dimi_RemovePlayerFromVehicle( playerid ); UsaoAutoProdaja[ playerid ] = -1;
	    if(GetPVarInt( playerid, "NoFuel") == 1) {
