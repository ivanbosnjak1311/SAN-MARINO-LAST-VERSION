			if( !IsPlayerConnected( id ) ) return SendErrorMessage( playerid, "Taj igrac je offline!");
			if( PlayerInfo[ id ][ xGradRank ] >= 1) return SendErrorMessage( playerid, "Taj igrac je vec u sluzbi gradonacelnika!");
